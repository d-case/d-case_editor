    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ApprovedBy" type="EM:ApprovedBy"/>
  <xsd:complexType name="CreatedBy">
    <xsd:complexContent>
      <xsd:extension base="EM:Provenance">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="source" type="EM:Stakeholder"/>
