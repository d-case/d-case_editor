        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="definition" type="xsd:anyURI"/>
        </xsd:choice>
        <xsd:attribute name="concept" type="xsd:string" use="required"/>
        <xsd:attribute name="definition" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
